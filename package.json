{
  "name": "rsreact",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "author": "Olga",
  "scripts": {
    "dev:server": "nodemon --watch server.ts --watch src/server --exec 'ts-node server.ts'",
    "dev:client": "yarn build:client && vite --config vite.config.ts dev",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "debug": "node --inspect-brk server",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext js,ts,tsx src",
    "build": "rimraf dist && tsc -p tsconfig.json && yarn build:client && yarn build:server && yarn copy-files",
    "build:client": "vite build --outDir dist --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "test:watch": "vitest --watch",
    "serve": "yarn build && cross-env NODE_ENV=production yarn ts-node ./server",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles src/assets/* dist/assets && copyfiles index.html dist/client && copyfiles -f dist/client/assets/* dist/assets",
    "test:cypress": "start-server-and-test start http://localhost:7456 cypress",
    "cypress": "cypress run",
    "cypress:o": "cypress open",
    "cover": "nyc --reporter=html --reporter=text-summary"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@jest/globals": "^29.5.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/compression": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@uniswap/widgets": "^2.51.1",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux": "^4.2.1",
    "serve-static": "^1.15.0",
    "vite-plugin-svgr": "^2.4.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.7",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false
  }
}
